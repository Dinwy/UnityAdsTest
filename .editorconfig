# EditorConfig is awesome: http://EditorConfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
trim_trailing_whitespace = false
charset = utf-8

[*.cs]
indent_style = tab
indent_size = 4

[*.py]
indent_style = space
indent_size = 4

[*.md]
indent_size = 2
trim_trailing_whitespace = false